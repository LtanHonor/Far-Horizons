
REP_OBJS = Report.o get_gal.o get_star.o get_plan.o utils.o

Report: $(REP_OBJS)
	cc $(REP_OBJS) -o ../bin/Report

Report.o: Report.c fh.h
	cc -c Report.c

utils.o: utils.c fh.h
	cc -c utils.c

get_gal.o: get_gal.c fh.h
	cc -c get_gal.c

get_plan.o: get_plan.c fh.h
	cc -c get_plan.c

get_star.o: get_star.c fh.h
	cc -c get_star.c
